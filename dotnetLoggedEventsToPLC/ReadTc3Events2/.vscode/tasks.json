{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/ReadTc3Events2.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "build-release",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/ReadTc3Events2.csproj",
                "-c",
                "Release",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "publish-freebsd",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/ReadTc3Events2.csproj",
                "-c",
                "Release",
                "-o",
                "${workspaceFolder}/../../build-artifacts/freebsd/ReadTc3Events2"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "publish-windows",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/ReadTc3Events2.csproj",
                "-c",
                "Release",
                "-r",
                "win-x64",
                "--self-contained",
                "-p:PublishSingleFile=true",
                "-o",
                "${workspaceFolder}/../../build-artifacts/windows/ReadTc3Events2"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "clean",
            "command": "dotnet",
            "type": "process",
            "args": [
                "clean",
                "${workspaceFolder}/ReadTc3Events2.csproj"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "restore",
            "command": "dotnet",
            "type": "process",
            "args": [
                "restore",
                "${workspaceFolder}/ReadTc3Events2.csproj"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "run-local-english",
            "command": "dotnet",
            "type": "process",
            "args": [
                "run",
                "--project",
                "${workspaceFolder}/ReadTc3Events2.csproj",
                "--",
                "--symbolpath",
                "MAIN.fbReadTc3Events",
                "--languageid",
                "1033",
                "--datetimeformat",
                "2"
            ],
            "group": "test",
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared"
            }
        },
        {
            "label": "run-verbose",
            "command": "dotnet",
            "type": "process",
            "args": [
                "run",
                "--project",
                "${workspaceFolder}/ReadTc3Events2.csproj",
                "--",
                "--symbolpath",
                "MAIN.fbReadTc3Events",
                "--languageid",
                "1033",
                "--datetimeformat",
                "2",
                "--verbose"
            ],
            "group": "test",
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared"
            }
        }
    ]
}